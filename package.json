{
    "name": "hain-plugin-gitlab",
    "version": "0.0.1",
    "description": "Hain plugin to query a GitLab Instance for projects you own or you are member of. Will open the project within a browser on execute.",
    "main": "index.js",
    "author": "Jan Froehlich <jf.1603@gmail.com>",
    "keywords": [
        "hain-0.3.0",
        "gitlab",
        "plugin"
    ],
    "hain": {
        "prefix": "/gitlab",
        "usage": "type /gitlab something to open project in gitlab",
        "icon": "#fa fa-gitlab",
        "redirect": "/gitlab "
    },
    "dependencies": {
        "node-gitlab": "^1.2.1"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jf1603/hain-plugin-gitlab.git"
    },
    "bugs": {
        "url": "https://github.com/jf1603/hain-plugin-gitlab/issues"
    },
    "homepage": "https://github.com/jf1603/hain-plugin-gitlab#readme"
}
